style.
    body.container{
        width:95%;
    }
    div.menu{
        position: fixed;
        background-color:white;
        left:0;
        top:0;
        bottom:0;
        //height:100%;
        overflow-y:scroll;
        overflow-x:hidden;
        width: 480px;
        z-index:1000;
        padding:10px 0px 10px 10px;
    }
    div.api{
        position:relative;
        margin: 20px;
    }
    div.quickMenu{
        position:fixed;
        right: 40px;
        top: 30px;
        z-index: 1000;

    }
    pre.codeWrapper{
        height: 100%;
        padding: 10px;
        margin: 2px;
        overflow: hidden;
    }
    h2.groupRoute{
        background-color: green;
        color: #fff;
        padding: 20px 40px;
    }
    h2.groupRoute a{
        color: #fff;
    }
    table.summary td{
        padding:2px 10px;
    }
    table.example{
        margin:15px auto;
        padding:10px;
        width:95%;
    }
    table.example td, table.example th{
        vertical-align:top;
        width: 50%;
    }
    div.summaryCategory{
        float:left;
        display: block;
        width:40%;
        padding-bottom: 20px;
    }
    div.summaryCategory h3{

    }
    div.example{
        clear:both;
    }